	for(var i=0;i<datas.length;i++)
	{
		if(datas[i].id == lifeRoadLast0 )
		{
			REPLACER(dataShow.reason,datas[i],lifeRoadLast1,name,age,sex);
			datas.splice(i,1);
			break;
		}
	}
	
